{
    "name": "krencl/wedos-api",
    "description": "Wedos API library",
	"version": "1.0",
    "type": "library",
    "authors": [
        {
            "name": "Krencl",
            "email": "krencl@gmail.com"
        }
    ],
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/krencl/wedos-api"
		}
	],
    "autoload": {
        "psr-0" : {
            "Krencl" : "src"
        }
    },
    "require": {
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3"
    },
	"scripts": {
		"test": "./vendor/bin/phpunit --bootstrap vendor/autoload.php"
	}
}
